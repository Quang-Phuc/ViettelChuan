	<main class="content">
	<div>
		<img src="images/banner.jpg" class="img-fluid"
			alt="Đăng ký Data 4G Viettel" />
	</div>
	<div class="container d-block d-sm-none">
		<div class="row">
			<form method="post" accept-charset="utf-8" style="width: 100%"
				action="https://data4gsieutoc.com/tim-khuyen-mai">
				<div style="display: none;">
					<input type="hidden" name="_method" value="POST" /><input
						type="hidden" name="_csrfToken" autocomplete="off"
						value="fd682d3b0046bfdc222cce9041439a1ea6bdfa8a9a82d2c80c64aaef551680807c58ff584c211a66b1816c5bd7b7747c1076dbfbe46ac011ddeec39ccedc4435" />
				</div>
				<div class="input-group my-3 col-12 text">
					<input type="text" name="phone_number" class="form-control"
						placeholder="Nhập số điện thoại để xem khuyến mại"
						id="phone-number" />
					<div class="input-group-append">
						<button class="input-group-text" id="basic-addon2">
							<i class="fas fa-search" aria-hidden="true"></i>
						</button>
					</div>
				</div>
			</form>
		</div>
	</div>
	<div class="container"></div>

	<c:forEach items="${listPacks}" var="listPacks" varStatus="vsp">
		<div class="category" style="background: #eee">
			<div class="container">
				<h2 class="text-center">
					<span style="color: #14928f; font-weight: bold"><c:out
							value="${listPacks.title}"></c:out></span>
				</h2>
				<h5 class="text-center">
					 <span style="color: #14928f; font-weight: bold"><c:out
							value="${listPacks.detail}"></c:out></span></span>
				</h5>
				<div class="row">

					<c:forEach items="${listPacks.packDataList}" var="paskData" varStatus="vs">
						<div class=col-md-3>
							<div class="product">
								<div class="product-title">
									<c:out value="${paskData.name}"></c:out>
								</div>
								<p hidden id="myParagraph${vsp.index}${vs.index}">
									<c:out value="${paskData.description}"></c:out>
								</p>
								
								<p>
									<script>
										var index = ${vs.index}
										var index2 = ${vsp.index}
										var theString = document
												.getElementById("myParagraph"+ index2 + index).innerHTML;
										var varTitle = $('<textarea />').html(
												theString).text();
										document.write(varTitle);
									</script>
								</p>
								<div class="detail">
									<button type="button" class="btn btn-outline-primary"
										data-toggle="modal" data-target="#detail-2-0${vsp.index}${vs.index}">
										Chi tiết <i class="far fa-file-alt"></i>
									</button>
									<div class="modal fade" id="detail-2-0${vsp.index}${vs.index}">
										<div class="modal-dialog">
											<div class="modal-content">
												<!-- Modal Header -->
												<div class="modal-header">
													<h6 class="modal-title">
														Gói cước
														<c:out value="${paskData.name}"></c:out>
													</h6>
													<button type="button" class="close" data-dismiss="modal">&times;</button>
												</div>

												<!-- Modal body -->
												<div class="modal-body">
													<p  hidden id="myParagrapht${vsp.index}${vs.index}">
														<c:out value="${paskData.detail}"></c:out>
													</p>
													<p>
														<script>
															var index = ${vs.index}
															var index2=${vsp.index}
															var theString = document
																	.getElementById("myParagrapht"+ index2+index).innerHTML;
															var varTitle = $(
																	'<textarea />')
																	.html(
																			theString)
																	.text();
															document
																	.write(varTitle);
														</script>
													</p>
												</div>

												<!-- Modal footer -->
												<div class="modal-footer">
													<button type="button" class="btn btn-danger"
														data-dismiss="modal">Đóng</button>
												</div>

											</div>
										</div>
									</div>
								</div>
								<div class="register">
									<button type="button" class="btn btn-primary"
										data-toggle="modal" data-target="#syntax-2-0${vsp.index}${vs.index}">
										Đăng ký <i class="fas fa-paper-plane"></i>
									</button>
									<div class="modal fade" id="syntax-2-0${vsp.index}${vs.index}">
										<div class="modal-dialog">
											<div class="modal-content">
												<!-- Modal Header -->
												<div class="modal-header">
													<h6>
														<i class="fas fa-help"></i>Hướng dẫn
													</h6>
													<button type="button" class="close" data-dismiss="modal">&times;</button>
												</div>

												<!-- Modal body -->
												<div class="modal-body">
													<p class="text-center">Quý khách hãy gửi tin nhắn đăng
														ký theo cú pháp:</p>
													<div class="syntax">
														<span style="color: #14928f; font-weight: bold"><c:out value="${paskData.contentSms}"></c:out></span> gửi <span
															style="color: #14928f; font-weight: bold"><c:out value="${paskData.phoneSms}"></c:out></span>
													</div>
												</div>

												<!-- Modal footer -->
												<div class="modal-footer">
													<button type="button" class="btn btn-danger"
														data-dismiss="modal">Đóng</button>
												</div>

											</div>
										</div>
									</div>

								</div>
							</div>
						</div>
					</c:forEach>











				</div>
